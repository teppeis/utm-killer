{
  "name": "utm-killer",
  "description": "Chrome extension to remove tracking params (like `utm_*`) from URL",
  "version": "0.0.2",
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "main": "nothing",
  "devDependencies": {
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-cache": "^0.4.3",
    "gulp-chrome-manifest": "^0.0.13",
    "gulp-eslint": "^2.0.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.2.1",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-size": "^2.1.0",
    "gulp-zip": "^3.0.2",
    "run-sequence": "^1.1.4"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "chrome": true
    },
    "rules": {
      "eol-last": 0,
      "quotes": [
        2,
        "single"
      ]
    }
  }
}
